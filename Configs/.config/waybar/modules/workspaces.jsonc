"hyprland/workspaces": {
	"format": "{icon}",
	"disable-scroll": true,
	"rotate": ${r_deg},
	"all-outputs": true,
	"active-only": false,
	"on-click": "activate",
	"disable-scroll": false,
	"on-scroll-up": "hyprctl dispatch workspace -1",
	"on-scroll-down": "hyprctl dispatch workspace +1",
	"format-icons": {
		"1": "󰈹",
		"2": "",
		"3": "",
		"4": "󰊴",
		"5": "",
		"6": "",
		"7": "",
		"8": "",
		"9": "IX"
	},
	"sort-by-number": true,
	"persistent-workspaces": {
	}
},
